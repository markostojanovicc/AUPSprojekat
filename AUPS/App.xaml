<Application x:Class="AUPS.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:AUPS"
             xmlns:converters="clr-namespace:AUPS.Converters">
    
    <Application.Resources>

        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/AUPS;component/Resources/DataTemplates.xaml"/>
                <ResourceDictionary Source="/AUPS;component/Resources/Styles.xaml"/>
                <ResourceDictionary Source="/AUPS;component/Resources/Images.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>

        
    </Application.Resources>
</Application>
