<UserControl x:Class="AUPS.Controls.MainContentUserControls.TehnoloskiPostupakControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:AUPS.Controls.MainContentUserControls"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <DataGrid AutoGenerateColumns="True" IsReadOnly="True" SelectedItem="{Binding ItemSelected}" SelectionMode="Single" Grid.Row="1" Grid.Column="1"  ItemsSource="{Binding TehnoloskiPostupakList}" HorizontalAlignment="Stretch" Margin="0,10,0,0" VerticalAlignment="Stretch"/>
    </Grid>
</UserControl>
